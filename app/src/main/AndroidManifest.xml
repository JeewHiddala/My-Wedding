<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mywedding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Vender">

        </activity>
        <activity android:name=".VenderList"
            android:parentActivityName=".Vender"
        />
        <activity android:name=".VenderPaidList"
            android:parentActivityName=".Vender"/>
        <activity android:name=".ViewVender"
            android:parentActivityName=".VenderList"/>
        <activity android:name=".EditVender"
            android:parentActivityName=".ViewVender">

        </activity>
        <activity android:name=".AddVender"
            android:parentActivityName=".VenderList"/>
        <activity android:name=".EditBudget" />
        <activity android:name=".AddBudget" />
        <activity android:name=".PaidList" />
        <activity
            android:name=".BudgetList"
            android:parentActivityName=".BudgetMenu" />
        <activity android:name=".BudgetMenu" />
        <activity
            android:name=".Registration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"> <!-- hides the action bar -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>