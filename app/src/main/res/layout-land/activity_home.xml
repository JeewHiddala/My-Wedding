<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bgPalePink"
    android:foregroundTint="#FFFFFF"
    tools:context=".Home">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:background="@color/navPink"
        android:divider="@drawable/welcomepage">

        <TextView
            android:id="@+id/brideName1"
            android:layout_width="301dp"
            android:layout_height="37dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:paddingStart="1dp"
            android:paddingTop="5dp"
            android:paddingEnd="5dp"
            android:paddingBottom="5dp"
            android:text="Bride and Groom"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="24sp"
            android:textStyle="normal|bold|italic" />


        <ImageButton
            android:id="@+id/settingBTN"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginStart="305dp"
            android:backgroundTint="@color/navPink"
            app:srcCompat="@drawable/icons8_gear_64" />

        <TextView
            android:id="@+id/wedDate1"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-650dp"
            android:layout_marginTop="55dp"
            android:layout_marginEnd="150dp"
            android:text="Wedding Date : "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/wedDate2"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-280dp"
            android:layout_marginTop="85dp"
            android:layout_marginEnd="400dp"
            android:text=" 20/10/2020 "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txtWeddingDays"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-360dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="200dp"
            android:text=" 00 "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtdots1"
            android:layout_width="18dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-200dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" : "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtWeddingHours"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" 00 "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtdots2"
            android:layout_width="18dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" : "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtWeddingMins"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" 00 "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtdots3"
            android:layout_width="18dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" : "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/txtWeddingSeconds"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="100dp"
            android:text=" 00 "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/dayhrms1"
            android:layout_width="253dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-355dp"
            android:layout_marginTop="90dp"
            android:layout_marginEnd="100dp"
            android:text=" Days : Hours : Mins : Secs "
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/logo1"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:layout_marginStart="-100dp"
            android:layout_marginTop="2dp"
            app:srcCompat="@drawable/mywedding_logo" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="136dp"
        android:layout_height="177dp"
        android:background="@color/bgPalePink"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/taskBtn1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:background="@color/bgPalePink"
            app:srcCompat="@drawable/icons8_survey_80" />

        <TextView
            android:id="@+id/task1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-110dp"
            android:layout_marginTop="140dp"
            android:layout_marginEnd="20dp"
            android:text="@string/btnTasks"
            android:textAlignment="center"
            android:textSize="24sp"
            android:textStyle="normal|bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="136dp"
        android:layout_height="177dp"
        android:background="@color/bgPalePink"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout6"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/guestBtn1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:background="@color/bgPalePink"
            app:srcCompat="@drawable/icons8_people_80" />

        <TextView
            android:id="@+id/guests1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-110dp"
            android:layout_marginTop="140dp"
            android:layout_marginEnd="20dp"
            android:text="@string/btnGuests"
            android:textAlignment="center"
            android:textSize="24sp"
            android:textStyle="normal|bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="136dp"
        android:layout_height="177dp"
        android:background="@color/bgPalePink"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.499"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.987">

        <ImageButton
            android:id="@+id/budgetBtn1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:background="@color/bgPalePink"
            app:srcCompat="@drawable/icons8_debt_80" />

        <TextView
            android:id="@+id/budget1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-110dp"
            android:layout_marginTop="140dp"
            android:layout_marginEnd="20dp"
            android:text="@string/btnBudget"
            android:textAlignment="center"
            android:textSize="24sp"
            android:textStyle="normal|bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="136dp"
        android:layout_height="177dp"
        android:background="@color/bgPalePink"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993">

        <ImageButton
            android:id="@+id/vendorsBtn1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:background="@color/bgPalePink"
            app:srcCompat="@drawable/icons8_small_business_80" />

        <TextView
            android:id="@+id/vendors1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-120dp"
            android:layout_marginTop="140dp"
            android:layout_marginEnd="18dp"
            android:text="@string/btnVendors"
            android:textAlignment="center"
            android:textSize="24sp"
            android:textStyle="normal|bold" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="136dp"
        android:layout_height="177dp"
        android:background="@color/bgPalePink"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993">

        <ImageButton
            android:id="@+id/dashboardBtn1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:background="@color/bgPalePink"
            app:srcCompat="@drawable/icons8_prototype_80" />

        <TextView
            android:id="@+id/dashboard1"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-130dp"
            android:layout_marginTop="140dp"
            android:layout_marginEnd="10dp"
            android:text="@string/btnDashboard"
            android:textAlignment="center"
            android:textSize="24sp"
            android:textStyle="normal|bold" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>